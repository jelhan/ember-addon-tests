{
  "name": "ember-addon-tests",
  "version": "0.0.0",
  "description": "Declarative integration tests for Ember Addons",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc",
    "lint": "eslint '*/**/*.{js,ts}'",
    "prepare": "tsc",
    "test": "mocha -r ts-node/register 'tests/**/*.ts'",
    "test:performance": "for file in perf-tests/*.ts; do ts-node ${file}; done"
  },
