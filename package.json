  "license": "MIT",
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "copy-dir": "^1.3.0",
    "debug": "^4.1.1",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/chai-fs": "^2.0.2",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.4",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-fs": "^2.0.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
